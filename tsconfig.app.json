/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@enums": ["src/app/shared/enums/index"],
      "@models": ["src/app/shared/models/index"],
      "@services": ["src/app/shared/services/index"],
      "@pages": ["src/app/pages/index"],
      "@utils": ["src/app/shared/utils/index"]
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"]
}
